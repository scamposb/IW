			<xs:element name="return" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="removeTask">
		<xs:sequence>
			<xs:element name="title" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="removeTaskResponse">
		<xs:sequence>
			<xs:element name="return" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
